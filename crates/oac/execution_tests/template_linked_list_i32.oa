instantiate IntList = LinkedList[I32]

fun main() -> I32 {
	xs = IntList.empty()
	xs = IntList.push_front(3, xs)
	xs = IntList.push_front(2, xs)
	xs = IntList.push_front(1, xs)

	print(IntList.length(xs))
	print(IntList.head_or(xs, 99))

	ys = IntList.tail_or(xs, IntList.empty())
	print(IntList.head_or(ys, 99))

	zs = IntList.tail_or(IntList.empty(), ys)
	print(IntList.length(zs))

	print(IntList.is_empty(IntList.empty()))
	print(IntList.is_empty(xs))

	return 0
}
